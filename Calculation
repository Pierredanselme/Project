def transcription(Ts):
    
    RNA = ''
    trans = {'A': 'U', 'T': 'A', 'C': 'G', 'G': 'C'}
    
    for base in TS:
        RNA += trans[base] 

    return RNA

def main(DNA):
    B = transcription(DNA)
    D_A = 'AATTCC'
    D_B = 'UUA'

    if D_A in DNA:
        print('You have the disease A')
    else:
        print('You do not have the disease A')



